<table>
  <% l = $.parseJSON(lang);
  _.each(Object.keys(l), function(key){
    %>
      <tr>
        <td class="country" data-country="<%= key %>"><%= key %></td>
        <td>
          <% _.each(l[key], function(lang){ %>
            <span class="language" data-country="<%= key %>" data-lang="<%= lang %>"><%= lang %></span>
          <% }); %>
        </td>
      </tr>
    <%
  }); %>
</table>